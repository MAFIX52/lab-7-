#include <iostream>
using namespace std;
int main()
{
	setlocale(0,"");
    int n1;
    cout << "Ведите число: ";
    cin >> n1;

    cout << "все делители числа: " << n1 << endl;
    for (int i = 1; i <= n1; i++) 
    {
        if (n1 % i == 0) 
        {

            cout << i << " ";
        }

    }
	return 0;
}
